<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mbc.shk010.member.MemberService">
	<insert id="insertm">
		insert into member0906 values(#{param1}, #{param2}, #{param3}, #{param4}, #{param5}, #{param6}, #{param7}, #{param8})
	</insert>
	
	<select id="outm" resultType="com.mbc.shk010.member.MemberDTO">
		select * from member0906
	</select>
	
	<select id="delviewm" resultType="com.mbc.shk010.member.MemberDTO">
		select * from member0906 where id=#{param1}
	</select>
	
	<delete id="delm">
		delete from member0906 where id=#{param1}
	</delete>
	
	<select id="updviewm" resultType="com.mbc.shk010.member.MemberDTO">
		select * from member0906 where id=#{param1}
	</select>
	
	<delete id="updatem">
		update member0906 set pw=#{param1}, name=#{param2}, phone=#{param3}, gender=#{param4}, address=#{param5}, hobby=#{param6}, hello=#{param7} where id=#{param8}
	</delete>
	
	<select id="smi" resultType="com.mbc.shk010.member.MemberDTO">
		select * from member0906 where id like #{param1}
	</select>
	
	<select id="smn" resultType="com.mbc.shk010.member.MemberDTO">
		select * from member0906 where name like #{param1}
	</select>
	
	<select id="smg" resultType="com.mbc.shk010.member.MemberDTO">
		select * from member0906 where gender like #{param1}
	</select>
	
	<select id="sma" resultType="com.mbc.shk010.member.MemberDTO">
		select * from member0906 where address like #{param1}
	</select>
	
	<select id="idch2"  resultType="int">
		select count(*) from member0906 where id=#{param1}
	</select>
	
</mapper>
